<table class="table table-striped table-hover table-condensed" width="100%">
    <tr>
        <th></th>
        <th>PO No</th>
        <th>PO Date</th>
        <th>Vendor</th>
        <th>Type</th>
    </tr>

    @foreach (var item in ViewBag.PopUpPO as IEnumerable<WebApp_AAJI.Models.purchaseOrderHeader.POpopUp>)
    {
        <tr>
            <td>
                <a href="#" class="glyphicon glyphicon-ok-sign" data-toggle="modal" onclick="copyToForm(item.poId, item.poDate, item.vendorId, item.vendorName, item.poUrgent);"></a>
            </td>
            <td>@Html.Label("poId", item.poId)</td>
            <td>@Html.Label("poDate", item.poDate)</td>
            @*<td>@Html.Label("vendorId", item.vendorId)</td>*@
            <td>@Html.Label("vendorName", item.vendorName)</td>
            <td>@Html.Label("poUrgent", item.poUrgent)</td>
        </tr>
    }
</table>
